{
  "name": "Staafe - Stay Safe",
  "short_name": "Staafe",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Search the Web but stay safe",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "permissions": ["storage", "tabs", "https://staafe.dmnchzl.dev/api/*"],
  "browser_action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    },
    "default_title": "Staafe",
    "default_popup": "index.html?history=true"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "dmnchzl@staafe"
    }
  },
  "web_accessible_resources": ["fonts/*.ttf", "icons/*.png"]
}
